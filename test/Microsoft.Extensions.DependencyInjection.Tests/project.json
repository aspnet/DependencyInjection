{
  "version": "1.0.0-*",
  "buildOptions": {
    "keyFile": "../../tools/Key.snk",
    "warningsAsErrors": true
  },
  "dependencies": {
    "dotnet-test-xunit": "1.0.0-rc3-000000-01",
    "Microsoft.AspNetCore.Testing": "1.0.1",
    "Microsoft.Extensions.DependencyInjection": "1.0.2",
    "Microsoft.Extensions.DependencyInjection.Specification.Tests": "1.0.2",
    "xunit": "2.1.0"
  },
  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "dnxcore50",
        "portable-net451+win8"
      ],
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.0.11",
          "type": "platform"
        },
        "System.Diagnostics.Process": "4.1.0"
      }
    },
    "net451": {
      "frameworkAssemblies": {
        "System.Runtime": {
          "type": "build"
        }
      },
      "dependencies": {
        "xunit.runner.console": "2.1.0"
      }
    }
  },
  "testRunner": "xunit"
}